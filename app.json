{
  "expo": {
    "name": "PetWalk",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/DogPaw.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationWhenInUseUsageDescription": "Aplikacja używa lokalizacji, aby śledzić spacery.",
        "NSLocationAlwaysUsageDescription": "Aplikacja potrzebuje dostępu do lokalizacji nawet w tle."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Dogpaw-cuate.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDbuUiVX1RWvrpXlADLxdn47B8jQZTU-h8"
        }
      },
      "package": "com.twiggiermaen21.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6a210f0f-fbca-4c73-ab13-d5766d2dec26"
      }
    },
    "owner": "twiggiermaen21"
  }
}